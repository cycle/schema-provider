{
    "name": "cycle/schema-provider",
    "type": "library",
    "license": "MIT",
    "description": "Cycle ORM Schema providers",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=8.0",
        "cycle/orm": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "vimeo/psalm": "^5.6"
    },
    "autoload": {
        "psr-4": {
            "Cycle\\Schema\\Provider\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cycle\\Schema\\Provider\\Tests\\": "tests/"
        }
    }
}
